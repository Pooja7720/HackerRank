#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    char c,str[50],str1[100];
    
    scanf("%c",&c);
    //fflush(stdin);
    scanf("%s",str);
    //fflush(stdin);
    scanf("\n");
    /*  TO INPUT A SENTENCE WE USE THE FOLLOWING LINE OF SCANF*/
    scanf("%[^\n]%*s",str1);
    printf("%c\n",c);
    printf("%s\n",str);
    printf("%s",str1);
    
  
    return 0;
}

