#include <stdio.h>
#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char str[1000],c='0';
    int arr[10],count=0;
    gets(str);
    for(int i=0;i<10;i++) // LOOP FROM 0 TO 9
    {
        count=0;
        for(int j=0;j<strlen(str);j++) // LOOP TILL END OF THE STRING
        {
            if(str[j] == c)
            {
                count+=1; // NUMBER IS PRESENT SO INCREMENT COUNT
            }
            
        }
        printf("%d ",count); // PRINT THE FREQUENCY OF EACH NUMBER
        c++; // INCREMENTING THE ASCII VALUE OF CHARACTER FROM 0 TILL 9
        
    }
    
    return 0;
}

